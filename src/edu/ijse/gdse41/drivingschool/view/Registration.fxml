<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane id="AnchorPane" fx:id="rootAnchor" prefHeight="700.0" prefWidth="1200.0" stylesheets="@../styles/allStyles.css" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ViewController.RegistrationController">
   <children>
      <AnchorPane fx:id="overAnchor" prefHeight="700.0" prefWidth="1200.0">
         <children>
            <ImageView fitHeight="700.0" fitWidth="1200.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../image/Registration.jpg" />
               </image>
            </ImageView>
            <TextField fx:id="txtAddmissionId" layoutX="75.0" layoutY="147.0" prefHeight="31.0" prefWidth="312.0" promptText="Addmission ID" />
            <TextField fx:id="txtNIC" layoutX="73.0" layoutY="290.0" prefHeight="31.0" prefWidth="370.0" promptText="NIC Number" />
            <ComboBox fx:id="comboLessonType" layoutX="73.0" layoutY="491.0" prefHeight="33.0" prefWidth="230.0" promptText="Lesson Type" />
            <TextField fx:id="txtTel1" layoutX="337.0" layoutY="380.0" prefHeight="23.0" prefWidth="230.0" promptText="Tel 1" />
            <TextField fx:id="txTel2" layoutX="73.0" layoutY="380.0" prefHeight="31.0" prefWidth="230.0" promptText="Tel 2" />
            <Button fx:id="bttback" mnemonicParsing="false" onAction="#bttBackClicked" prefHeight="40.0" prefWidth="40.0" styleClass="backButt" />
            <RadioButton fx:id="rdoMale" layoutX="73.0" layoutY="335.0" mnemonicParsing="false" text="Male" textFill="WHITE" />
            <RadioButton fx:id="rdoFemale" layoutX="168.0" layoutY="335.0" mnemonicParsing="false" text="Female" textFill="WHITE" />
            <HBox layoutX="1066.0" layoutY="14.0" prefHeight="31.0" prefWidth="120.0" spacing="10.0">
               <children>
                  <Button fx:id="bttMini" mnemonicParsing="false" onAction="#bttMiniClicked" prefHeight="32.0" prefWidth="32.0" styleClass="windowBtns" />
                  <Button fx:id="bttMax" layoutX="10.0" layoutY="10.0" mnemonicParsing="false" onAction="#bttMaxClicked" prefHeight="32.0" prefWidth="32.0" styleClass="windowBtns" />
                  <Button fx:id="bttClose" layoutX="75.0" layoutY="10.0" mnemonicParsing="false" onAction="#bttCloseClicked" prefHeight="32.0" prefWidth="32.0" styleClass="windowBtns" />
               </children>
            </HBox>
            <HBox layoutX="795.0" layoutY="577.0" prefHeight="53.0" prefWidth="331.0" spacing="20.0">
               <children>
                  <JFXButton fx:id="bttCancel" buttonType="RAISED" onAction="#bttCancelClicked" prefHeight="45.0" prefWidth="150.0" styleClass="cancelControllButtonn" text="Cancel" />
                  <JFXButton fx:id="bttNext" buttonType="RAISED" onAction="#bttNextClicked" prefHeight="45.0" prefWidth="150.0" styleClass="controlleButtons" text="Next" />
               </children>
            </HBox>
            <VBox layoutX="73.0" layoutY="192.0" prefHeight="98.0" prefWidth="779.0" spacing="20.0">
               <children>
                  <TextField fx:id="txtFullName" promptText="Full Name" />
                  <TextField fx:id="txtInitialName" layoutX="10.0" layoutY="10.0" prefWidth="779.0" promptText="Name with Initials" styleClass="text-field" />
               </children>
            </VBox>
            <ComboBox fx:id="comboVehicalClass" layoutX="73.0" layoutY="433.0" prefHeight="33.0" prefWidth="230.0" promptText="Vehicle Class" />
            <Label fx:id="lblSourse" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
